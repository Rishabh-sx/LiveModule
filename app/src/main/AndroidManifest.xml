<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rishabh.livemodule">

    <application
        android:name=".ApplicationLive"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".live.live.liveCamera.LiveCameraActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".live.player.playeroverlay.PlayerOverlayActivity" />
        <activity android:name=".live.player.videoplayer.VideoPlayerActivity" />
        <activity android:name=".live.live.cameraOverlay.CameraOverlayActivity" />
    </application>

</manifest>